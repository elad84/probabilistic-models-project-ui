{
  "name": "probabilistic-model-sdk",
  "version": "0.0.1",
  "description": "Visualization and action sdk",
  "main": "./src/server.js",
  "engines": {
    "node": "^7",
    "npm": "^3"
  },
  "private": true,
  "scripts": {
    "dev": "better-npm-run deploy:dev",
    "test": "mocha",
    "build:dev": "better-npm-run compile",
    "build:prod": "better-npm-run build:prod",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy"
  },
  "betterScripts": {
    "compile": "webpack --config $npm_package_webpack --progress --colors",
    "build:prod": {
      "command": "webpack --config $npm_package_webpack --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:dev": {
      "command": "npm run build:dev && nodemon ./src/server.js",
      "env": {
        "DEBUG": "express:*"
      }
    },
    "deploy": {
      "command": "nodemon ./src/server.js",
      "env": {
        "DEBUG": "express:*"
      }
    }
  },
  "author": "eladcohen",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "better-npm-run": "0.0.13",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "compression": "^1.6.2",
    "datatables": "^1.10.13",
    "datatables.net": "^1.10.13",
    "datatables.net-dt": "^1.10.13",
    "express": "^4.14.0",
    "vis": "^4.17.0"
  }
}
